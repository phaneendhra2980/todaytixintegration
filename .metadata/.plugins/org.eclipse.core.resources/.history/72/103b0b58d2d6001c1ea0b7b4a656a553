package com.checkoutjourney;

import org.testng.annotations.Test;

import io.restassured.RestAssured;
import io.restassured.http.ContentType;

import static io.restassured.RestAssured.*;
import static variables.Configuration.BASE_API_URL;

public class Test01_Get {
	
	
	
	
  @Test
  public void getCheckOutJourney() {
	  
	  RestAssured.baseURI = BASE_API_URL;
	  given().contentType(ContentType.JSON).header("affiliateId", "encoretickets") .when().get(BASE_API_URL).then().log()
      .body();
	  
	  
  }
}
