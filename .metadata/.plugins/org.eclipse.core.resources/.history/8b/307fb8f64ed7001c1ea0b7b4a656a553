package models;

import java.util.ArrayList;


public class Areas {
	 Request RequestObject;
	 Response ResponseObject;
	 private String context = null;


	 // Getter Methods 

	 public Request getRequest() {
	  return RequestObject;
	 }

	 public Response getResponse() {
	  return ResponseObject;
	 }

	 public String getContext() {
	  return context;
	 }

	 // Setter Methods 

	 public void setRequest(Request requestObject) {
	  this.RequestObject = requestObject;
	 }

	 public void setResponse(Response responseObject) {
	  this.ResponseObject = responseObject;
	 }

	 public void setContext(String context) {
	  this.context = context;
	 }
	}
	
	public class Request {
	 private String body;
	 Query QueryObject;
	 UrlParams UrlParamsObject;


	 // Getter Methods 

	 public String getBody() {
	  return body;
	 }

	 public Query getQuery() {
	  return QueryObject;
	 }

	 public UrlParams getUrlParams() {
	  return UrlParamsObject;
	 }

	 // Setter Methods 

	 public void setBody(String body) {
	  this.body = body;
	 }

	 public void setQuery(Query queryObject) {
	  this.QueryObject = queryObject;
	 }

	 public void setUrlParams(UrlParams urlParamsObject) {
	  this.UrlParamsObject = urlParamsObject;
	 }
	}
	
	
